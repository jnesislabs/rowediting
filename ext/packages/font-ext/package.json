{
    "name": "@extjs/ext-react-font-ext",
    "version": "6.5.0.775",
    "sencha": {
        "name": "font-ext",
        "namespace": "Ext",
        "type": "code",
        "framework": "ext",
        "toolkit": "classic",
        "theme": "theme-neptune",
        "creator": "Sencha",
        "summary": "Extra font icons",
        "detailedDescription": "Simple web font with additional icons in the Font Awesome style",
        "version": "6.5.0",
        "compatVersion": "6.0.0",
        "format": "1",
        "output": "${framework.dir}/build/packages/${package.name}",
        "local": true,
        "resource": {
            "paths": [
                "${package.dir}/resources"
            ]
        },
        "resources": [
            {
                "path": "${package.dir}/resources",
                "output": "shared"
            }
        ],
        "sass": {
            "namespace": "Ext",
            "etc": "${package.dir}/sass/etc/all.scss",
            "var": "${package.dir}/sass/var",
            "src": [
                "${package.dir}/sass/src",
                "${package.dir}/sass/src/all.scss"
            ]
        },
        "classpath": "${package.dir}/src",
        "overrides": "${package.dir}/overrides",
        "slicer": null
    }
}